# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Clone a remote repository
GET     /cloneRepo                  controllers.JGit.cloneRepo()
GET     /link                  		controllers.Identity.linkForm(hashUUID:String)
POST    /jgit/:uuid/displayBranch	controllers.JGit.displayBranch(uuid:String, studentname:String)
GET     /handleRequest      		controllers.Identity.linkIdentity(username:String, hashUUID:String, mail:String)
GET     /students      				controllers.Application.students()
POST    /student/:uuid/delete       controllers.Application.deleteStudent(uuid:String)